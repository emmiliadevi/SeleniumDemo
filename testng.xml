<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QA Click Academy">
<listeners>
	<listener class-name="Academy.getListeners"/>
	<listener class-name="resources.ExtentReporterNG"/>
</listeners>
  <test thread-count="5" name="Verify Login Access">
    <classes>
      <class name="Academy.VerifyLoginAccess"/>
      </classes>
      </test>
       <test thread-count="5" name="Navigate to Login page">
        <classes>
      <class name="Academy.HomePageTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
